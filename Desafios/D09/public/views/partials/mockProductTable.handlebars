<h3 class="tituloSeccion">Productos Test</h3>
<label class="formLabel">Ingrese la cantidad de productos</label>
<input
  class="formInput"
  type="number"
  placeholder="cantidad de productos"
  id="testProductsQty"
  name="testProductsQty"
/>

<button id="btn__testProducts" class="btn__submit" onclick="getTest()">Obtener
  productos-test</button>
{{#if testProducts.length}}
  <table class="productTable">
    <tr>
      <th>Nombre</th>
      <th>Precio</th>
      <th>Im√°gen</th>
    </tr>
    {{#each testProducts}}
      <tr>
        <td>
          {{this.title}}
        </td>
        <td>
          {{this.price}}
        </td>
        <td>
          <img src={{this.thumbnail}} alt="" width="250" height="250" />
        </td>
      </tr>
    {{/each}}
  </table>
{{else}}
  <h3>no hay productos test en el sistema actualmente...</h3>
{{/if}}